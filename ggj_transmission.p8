--game objects
planet={}
function planet:new(o)
 o = o or {}
 setmetatable(o, self)
 self.__index = self
 return o
end

function planet:init(x,y,t)
 self.x=x
 self.y=y
 self._type=t
 self.sprite=5
 if(self._type==1) then
  self.sprite=5 --gas
 elseif(self._type==2) then
  self.sprite=7 --earth
 elseif(self._type==3) then
  self.sprite=9 --rock
 end
end

function planet:draw()
 spr(self.sprite,self.x,self.y)
 spr(self.sprite+1,self.x+8,self.y)
 spr(self.sprite+16,self.x,self.y+8)
 spr(self.sprite+17,self.x+8,self.y+8)
end

function planet:update(dt)
 --todo
end

enemy_shot={
speed=2
}

function enemy_shot:new(o)
 o = o or {}
 setmetatable(o, self)
 self.__index = self
 return o
end

function enemy_shot:init(x,y)
 self.x=x
 self.y=y
 self.width=2
 self.height=2
 self.velx=0
 self.vely=0
 self.lifetime=1.5
 self.timer=0
 self.c=8 --white
end

function enemy_shot:set_vel(x,y)
 self.velx=x
 self.vely=y
end

function enemy_shot:draw()
 rectfill(self.x,self.y,
 self.x+self.width-1,
 self.y+self.height-1,self.c)
end

function enemy_shot:update(dt)
 self.x+=self.velx
 self.y+=self.vely
 self.timer+=dt
 if(self.timer>self.lifetime) then
  self.timer=0
  del(enemy_shots,self)
 end
 if coll_obj(self,pship) then
  if(not pship.is_dying and not pship.is_warping) then
  	pship:kill()
   del(enemy_shots,self)
  end
 end
end

enemy={}
function enemy:new(o)
 o = o or {}
 setmetatable(o, self)
 self.__index = self
 return o
end

function enemy:init(x,y)
 self.x=x
 self.y=y
 self.first_x=x
 self.first_y=y
 self.width=16
 self.height=16
 self.y+=flr(rnd(2))-1
 self.hover_vel=1
 self.timer=flr(rnd(10))/10
 self.shield_timer=0
 self.shield_color1=12 --blue
 self.shield_color2=7 --white
 self.shield_color=12
 self.shield_color_timer=0
 self.shields_up=true
 self.dist_to_player=0
 self.hp=10
 --top-left=sprite
 --top-right=+1
 --bottom-left=+16
 --bottom-right=+17
 self.sprite=11
end

function enemy:set_type(t)
 if(t==1) then
  self.sprite=11
  self.hp=10
 elseif(t==2) then
  self.sprite=13
  self.hp=20
 end
end

function enemy:draw()
 spr(self.sprite,self.x,self.y)
 spr(self.sprite+1,self.x+8,self.y)
 spr(self.sprite+16,self.x,self.y+8)
 spr(self.sprite+17,self.x+8,self.y+8)
 if(self.shields_up) then
  circ(self.x+7,self.y+7,
  10,self.shield_color)
  circ(self.x+8,self.y+7,
  10,self.shield_color)
  circ(self.x+7,self.y+8,
  10,self.shield_color)
  circ(self.x+8,self.y+8,
  10,self.shield_color)
 end
end

function enemy:damage()
 if(self.hp>0) then
  self.hp-=1
 else
  for i=1,30 do
   local p=particle:new()
   local px=self.x+4
   local py=self.y+4
   local p_life=rnd(1)
   local p_size=flr(rnd(2))+1
   local p_color=flr(rnd(4))+1
   if(p_color==1) then
    p_color=6 --light gray
   elseif(p_color==2) then
    p_color=7 --white
   elseif(p_color==3) then
    p_color=11 --green
   elseif(p_color==4) then
    p_color=3
   end
   local p_velx=rnd(2)-1
   local p_vely=rnd(2)-1
   p:init(px,py,p_life)
   p:set_size(p_size,p_size)
   p:set_color(p_color)
   p:set_vel(p_velx,p_vely)
  add(particles,p)
 end
  del(enemies,self)
  sfx(1)
 end
end

function enemy:shoot()
 local x=self.x+7
 local y=self.y+7
 local es1=enemy_shot:new()
 es1:init(x,y)
 es1:set_vel(0,enemy_shot.speed)
 add(enemy_shots,es1)
 local es2=enemy_shot:new()
 es2:init(x,y)
 es2:set_vel(0,-enemy_shot.speed)
 add(enemy_shots,es2)
 local es3=enemy_shot:new()
 es3:init(x,y)
 es3:set_vel(-enemy_shot.speed,0)
 add(enemy_shots,es3)
 local es4=enemy_shot:new()
 es4:init(x,y)
 es4:set_vel(enemy_shot.speed,0)
 add(enemy_shots,es4)
end

function enemy:update(dt)
 self.timer+=dt
 if(self.timer>=0.2) then
  self.timer=0
  self.y+=self.hover_vel
  if(abs(self.y-self.first_y)>=1) then
   self.hover_vel*=-1
  end
 end
 
 --don't continue if player too
 --far away
 if(distance(self.x,self.y,pship.x,pship.y)>128) then
  return
 end
 
 self.shield_timer+=dt
 if(self.shield_timer>2) then
  self.shield_timer=0
  self:shoot()
  self.shields_up=not self.shields_up
 end
 
 if(self.shields_up) then  
  self.shield_color_timer+=dt
  if(self.shield_color_timer>0.1) then
   self.shield_color_timer=0
   if(self.shield_color==self.shield_color1) then
    self.shield_color=self.shield_color2
   elseif(self.shield_color==self.shield_color2) then
    self.shield_color=self.shield_color1
   end
  end
 end
end

shot={
speed=1
}

function shot:new(o)
 o = o or {}
 setmetatable(o, self)
 self.__index = self
 return o
end

function shot:init(x,y)
 self.x=x
 self.y=y
 self.width=2
 self.height=2
 --1=horizontal,2=vertical
 self.direction=1
 self.velx=0
 self.vely=0
 self.lifetime=1
 self.timer=0
 self.c=7 --white
end

function shot:set_vel(x,y)
 self.velx=x
 self.vely=y
end

function shot:draw()
 if(self.direction==1) then
  rectfill(self.x,self.y,
  self.x+self.width,
  self.y+(self.height/2),self.c)
 elseif(self.direction==2) then
  rectfill(self.x,self.y,
  self.x+(self.width/2),
  self.y+self.height,self.c)
 end
end

function shot:update(dt)
 self.x+=self.velx
 self.y+=self.vely
 self.timer+=dt
 if(self.timer>self.lifetime) then
  self.timer=0
  del(shots,self)
 end
 for e in all(enemies) do
  if coll_obj(self,e) then
   if(not e.shields_up) then
    e:damage()
    sfx(5)
   else
    sfx(4)
   end
   del(shots,self)
  end
 end
end

player={
speed=2,
up=1,
left=2,
down=3,
right=4
}

function player:new(o)
 o = o or {}
 setmetatable(o, self)
 self.__index = self
 return o
end

function player:init()
 self.x=-4
 self.y=-4
 self.width=8
 self.height=8
 self.sprite=player.up
 self.facing=player.up
 self.is_warping=false
 self.velx=0
 self.vely=0
 self.dir_stack={}
 self.particle_timer=0
 self.last_time=time()
 self.is_dying=false
 self.dead_timer=0
 self.lives=3
end

function player:draw()
 if not self.is_dying then
  spr(self.sprite, self.x, self.y)
 end
 if not game_over then
  for i=1,self.lives do
   spr(1,self.x-68+(i*10),self.y+52)
  end
 end
end

function player:shoot()
 sfx(0)
 local sh=shot:new()
 sh:init(self.x+(self.width/2),
 self.y+(self.height/2))
 sh.x-=sh.width/2
 sh.y-=sh.height/2
 if(self.facing==player.up) then
  sh.direction=2
  sh:set_vel(0,-player.speed*2)
 elseif(self.facing==player.down) then
  sh.direction=2
  sh:set_vel(0,player.speed*2)
 elseif(self.facing==player.left) then
  sh.direction=1
  sh:set_vel(-player.speed*2,0)
 elseif(self.facing==player.right) then
  sh.direction=1
  sh:set_vel(player.speed*2,0)
 end
 add(shots,sh)
end

function player:kill()
 sfx(3)
 self.is_dying=true
 for i=1,30 do
  local p=particle:new()
  local px=self.x+4
  local py=self.y+4
  local p_life=rnd(2)
  local p_size=flr(rnd(4))+1
  local p_color=flr(rnd(4))+7
  local p_velx=rnd(2)-1
  local p_vely=rnd(2)-1
  p:init(px,py,p_life)
  p:set_size(p_size,p_size)
  p:set_color(p_color)
  p:set_vel(p_velx,p_vely)
  add(particles,p)
 end
end

function player:update()
 local _t=time()
 local dt=_t-self.last_time
 self.last_time=_t
 --movement
 self.velx=0
 self.vely=0
 local num_pressed=0
 if btn(0) then
  num_pressed += 1
  add(self.dir_stack, player.left)
 end
 if btn(1) then
  num_pressed += 1
  add(self.dir_stack, player.right)
 end
 if btn(2) then
  num_pressed += 1
  add(self.dir_stack, player.up)
 end
 if btn(3) then
  num_pressed += 1
  add(self.dir_stack, player.down)
 end
 
 if(#self.dir_stack>8) then
  for item in all(self.dir_stack) do
   if(#self.dir_stack>8) then
    del(self.dir_stack, item)
   else
    break
   end
  end
 end
 
 d=self.dir_stack[#self.dir_stack]
 if(d==player.left) then
  self.velx = -player.speed
  self.sprite=d
 elseif(d==player.right) then
  self.velx = player.speed
  self.sprite=d
 elseif(d==player.up) then
  self.vely = -player.speed
  self.sprite=d
 elseif(d==player.down) then
  self.vely = player.speed
  self.sprite=d
 end
 
 local nextx=self.x+self.velx
 local nexty=self.y+self.vely
 --collision with enemies
 for e in all(enemies) do
  if coll_obj(self,e) then
   if not self.is_dying then
    self:kill()
   end
  end
 end
 --move only if any dir pressed
 if(num_pressed>0 and not self.is_warping and not self.is_dying) then
  self.x=nextx
  self.y=nexty
  self.facing=d
  --particle spawning in seconds
  self.particle_timer+=dt
  if(self.particle_timer>0.04) then
   self.particle_timer=0
   local p=particle:new()
   local px=self.x+(self.width/2)
   local py=self.y+(self.height/2)
   px+=rnd(4)-2
   py+=rnd(4)-2
   local p_life=rnd(0.8)
   local p_size=flr(rnd(2))+1
   local p_color=flr(rnd(3))+8
   local p_velx=-self.velx/4
   local p_vely=-self.vely/4
   p:init(px,py,p_life)
   p:set_size(p_size,p_size)
   p:set_color(p_color)
   p:set_vel(p_velx,p_vely)
   add(particles,p)
  end
  
  --warping
  if(self.x < left_edge or
  self.x > right_edge or
  self.y < top_edge or
  self.y > bottom_edge) then
   self.is_warping=true
  end
 end
 
 --shooting
 if btnp(4) then
  if not self.is_dying then
   self:shoot()
  end
 end
 
 if self.is_dying then
  self.dead_timer+=dt
  if(self.dead_timer>3) then
   self.dead_timer=0
   self.is_dying=false
   self.x=-4
   self.y=-4
   if(self.lives>1) then
    self.lives-=1
   else
    game_over=true
   end
  end
 end
end
