--main
world_x=0
world_y=0
left_edge=-120
right_edge=120
top_edge=-120
bottom_edge=120

is_fading=false
last_time=time()
dt=0

game_over=false
--1=title, 2=gameplay
game_state=1

world_map={}
map_x=90
map_y=6
map_w=32
map_h=32
pdot_timer=0
pdot_color=7

function draw_map()
 rectfill(world_x+map_x,
 world_y+map_y,
 world_x+map_x+map_w,
 world_y+map_y+map_h,0)
 rect(world_x+map_x,
 world_y+map_y,
 world_x+map_x+map_w,
 world_y+map_y+map_h,11)
 --player dot
 pdot_timer+=dt
 if(pdot_timer>=0.2) then
  pdot_timer=0
  if(pdot_color==7) then
   pdot_color=8 --red
  elseif(pdot_color==8) then
   pdot_color=7 --white
  end
 end
 local px=(pship.x/8)+16
 local py=(pship.y/8)+16
 rectfill(world_x+map_x+px,
 world_y+map_y+py,
 world_x+map_x+px,
 world_y+map_y+py,pdot_color)
 --enemy dots
 for e in all(enemies) do
  local ex=(e.first_x/8)+16
  local ey=(e.first_y/8)+16
  rectfill(world_x+map_x+ex,
  world_y+map_y+ey,
  world_x+map_x+ex,
  world_y+map_y+ey,10) --yellow
 end
 --planet dots
 for pl in all(planets) do
  local px=(pl.x/8)+16
  local py=(pl.y/8)+16
  rectfill(world_x+map_x+px,
  world_y+map_y+py,
  world_x+map_x+px,
  world_y+map_y+py,12) --blue
 end
end

stars={}
function init_stars(n)
 for i=1,n do
  add(stars,{
   x=flr(rnd(384)-192),
   y=flr(rnd(384)-192),
   size=flr(rnd(2)-0.5)
  })
 end
end

function draw_stars()
 for s in all(stars) do
  circfill(s.x,s.y,0,7)
 end
end

function start_game()
 game_over=false
 pship = player:new()
 pship:init()
 shots={}
 enemy_shots={}
 particles={}
 stars={}
 init_stars(150)
 enemies={}
 spawn_enemies()
 planets={}
 spawn_planets()
end

function spawn_enemies()
 for i=1,32 do
  for j=1,32 do
   local tile=mget(i,j)
   --enemy
   if(tile==11 or tile==13) then
    local e = enemy:new()
    e:init(i*8-128,j*8-128)
    if(tile==11) then
     e:set_type(1)
    elseif(tile==13) then
     e:set_type(2)
    end
    add(enemies,e)
   end
  end
 end
end

function spawn_planets()
 
end

function _init()
 show_ggj_logo(34,2,10)
 start_game()
end

function _draw()
 cls()
 if(game_state==1) then
  print("title screen",0,0)
  return
 end
 --bg rect
 rectfill(world_x,world_y,
 world_x+128,world_y+128,1)
 draw_stars()
 --draw world corners
 map(0,0,-128,-128,33,33,1)
 for pl in all(planets) do
  pl:draw()
 end
 for e in all(enemies) do
  e:draw()
 end
 for p in all(particles) do
  p:draw()
 end
 for sh in all(shots) do
  sh:draw()
 end
 for esh in all(enemy_shots) do
  esh:draw()
 end
 draw_borders()
 pship:draw()
 
 if(is_fading) then
  fadeout(60)
  for pix in all(fade_pixels) do
   rectfill(world_x+pix.x,
   world_y+pix.y,
   world_x+pix.x+pix.w,
   world_y+pix.y+pix.h,pix.c)
   rect(world_x+pix.x,
   world_y+pix.y,
   world_x+pix.x+pix.w,
   world_y+pix.y+pix.h,0)
  end
  if(not is_fading) then
   pship.is_warping=false
   pship.x=0
   pship.y=0
  end
 end
 draw_map()
 
 if game_over then
  local mx=pship.x-16
  local my=pship.y-8
  for i=0,2 do
   for j=0,2 do
    print("game over!",mx+i,my+j,0)
   end
  end
  print("game over!",mx+1,my+1,7)
 end
end

function _update()
 local _t=time()
 dt=_t-last_time
 last_time=_t
 if(game_state==1) then
  if btnp(4) then
   game_state=2
   camera(pship.x-64,pship.y-64)
  end
  return
 end
 if game_over then
  if btnp(4) then
   game_state=1
   camera(0,0)
   start_game()
  end
  return
 end
 for p in all(particles) do
  p:update()
 end
 for e in all(enemies) do
  e:update(dt)
 end
 for pl in all(planets) do
  pl:update(dt)
 end
 pship:update()
 for sh in all(shots) do
  sh:update(dt)
 end
 for esh in all(enemy_shots) do
  esh:update(dt)
 end
 update_borders(dt)
 
 camera(pship.x-64,pship.y-64)
 world_x=pship.x-64
 world_y=pship.y-64
 if(pship.is_warping) then
  if not is_fading then
   is_fading = true
   sfx(2)
   clear_list(fade_pixels)
  end
 end
end

