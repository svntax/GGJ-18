--misc
border_timer=0
border_horizontal=16
border_vertical=18
border_change=1
function draw_borders()
 for i=-15,15 do
  spr(border_horizontal,i*8,-128)
  spr(border_horizontal,i*8,128)
 end
 for i=-15,15 do
  spr(border_vertical,-128,i*8)
  spr(border_vertical,128,i*8)
 end
end

function update_borders(dt)
 border_timer+=dt
 if(border_timer>0.1) then
  border_timer=0
  border_horizontal+=border_change
  border_vertical+=border_change
  border_change*=-1
 end
end

fadecount=0
fade_pixels={}
function fadeout(limit)
 fadecount+=1
 if(fadecount>=limit) then
  is_fading=false
  fadecount=0
 end
 for i=1,8 do
  local col=flr(rnd(2))
  if col==0 then
   col=3 --dark green
  elseif col==1 then
   col=11 --light green
  end
  add(fade_pixels, {
   x=flr(rnd(128)),
   y=flr(rnd(128)),
   w=8,
   h=8,
   c=col
  })
 end
end

function clear_list(l)
 for e in all(l) do
  del(l, e)
 end
end

function coll_obj(o1,o2)
 return (o1.x<=o2.x+o2.width) and
 (o1.x+o1.width>=o2.x) and
 (o1.y<=o2.y+o2.height) and
 (o1.y+o1.height>=o2.y)
end

function distance(x1,y1,x2,y2)
 return sqrt((x2-x1)*(x2-x1)+(y2-y1)*(y2-y1))
end

particle={}

function particle:new(o)
 o = o or {}
 setmetatable(o, self)
 self.__index = self
 return o
end

function particle:init(x,y,t)
 self.x=x
 self.y=y
 self.w=4
 self.h=4
 self.c=7 --default white
 self.velx=0
 self.vely=0
 self.sprite=0
 self.lifetime=t
 self.timer=0
 self.last_time=time()
end

function particle:set_size(w,h)
 self.w=w
 self.h=h
end

function particle:set_vel(x,y)
 self.velx=x
 self.vely=y
end

function particle:set_sprite(s)
 self.sprite=s
end

function particle:set_color(c)
 self.c=c
end

function particle:draw()
 if(self.sprite~=0) then
  spr(self.sprite,world_x+self.x,world_y+self.y)
 else
  local x1=self.x - (self.w/2)
  local y1=self.y - (self.h/2)
  local x2=x1+self.w
  local y2=y1+self.h
  rectfill(x1,y1,x2,y2,self.c)
 end
end

function particle:update()
 local _t=time()
 local dt=_t-self.last_time
 self.last_time=_t
 self.x+=self.velx
 self.y+=self.vely
 self.timer+=dt
 if(self.timer>=self.lifetime) then
  del(particles,self)
 end
end
